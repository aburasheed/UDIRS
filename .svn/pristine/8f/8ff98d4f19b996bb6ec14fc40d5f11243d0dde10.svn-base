/*----------------------  Menu Button   --------------------------*/

.ios-6-arrow.blue {
  display: -moz-inline-stack;
  display: inline-block;
  vertical-align: middle;
  vertical-align: auto;
  zoom: 1;
  display: inline;
  -webkit-appearance: none;
  -moz-appearance: none;
  background: none;
  position: relative;
  margin: 0;
  padding: 0;
  border: 0;
  height: 26px;
  min-width: 20px;
  cursor: pointer;
  overflow: visible;
}


